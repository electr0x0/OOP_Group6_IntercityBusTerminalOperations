<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPaneShow" prefHeight="790.0" prefWidth="1172.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.busterminal.controller.MaintenanceStaff.GuideLineController">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="810.0" prefWidth="1210.0">
         <top>
            <MenuBar prefHeight="32.0" prefWidth="1185.0" BorderPane.alignment="CENTER">
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#newMenuItemsClick" text="New" />
                        <MenuItem mnemonicParsing="false" onAction="#openMenuItemsClick" text="Open" />
                        <MenuItem mnemonicParsing="false" onAction="#saveMenuItemsClick" text="Save" />
                        <MenuItem mnemonicParsing="false" onAction="#closeMenuItemsClick" text="Close" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="View">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#previousGuidelineMenuItemsClick" text="Previous Guidline" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
         </top>
         <center>
            <TextArea fx:id="textArea" prefHeight="730.0" prefWidth="1214.0" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
            </TextArea>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#sendOnMouseClick" prefHeight="43.0" prefWidth="150.0" text="Send" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
