<AnchorPane id="AnchorPane" prefHeight="488.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.busterminal.controller.passenger.ReservedBusListController">
   <children>
      <TableView fx:id="tableViewReserveBus" layoutX="25.0" layoutY="144.0" prefHeight="200.0" prefWidth="590.0">
        <columns>
          <TableColumn fx:id="busTypeCol" prefWidth="113.0" text="Bus Type" />
          <TableColumn fx:id="dateCol" minWidth="0.0" prefWidth="96.0" text="Date" />
            <TableColumn fx:id="acCol" prefWidth="64.0" text="AC" />
            <TableColumn fx:id="cityCol" prefWidth="88.0" text="City" />
            <TableColumn fx:id="durationCol" prefWidth="122.0" text="Duration (days)" />
            <TableColumn fx:id="fareCol" prefWidth="106.0" text="Fare" />
        </columns>
      </TableView>
      <Label layoutX="231.0" layoutY="70.0" prefHeight="31.0" prefWidth="224.0" text="Available Buses">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button layoutX="260.0" layoutY="396.0" mnemonicParsing="false" onAction="#reserveConfirmAlert" prefHeight="25.0" prefWidth="137.0" text="Select &amp; Confirm">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="572.0" layoutY="449.0" mnemonicParsing="false" onAction="#switchToReserveBusSceneOnClick" prefHeight="25.0" prefWidth="67.0" text="Back" />
   </children>
