<AnchorPane id="AnchorPane" prefHeight="488.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.busterminal.controller.driver.BusInfoController">
   <children>
      <Label layoutX="235.0" layoutY="36.0" prefHeight="56.0" prefWidth="219.0" text="Bus Information">
