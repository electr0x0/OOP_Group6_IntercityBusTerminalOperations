            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewBusInfo" layoutX="66.0" layoutY="122.0" prefHeight="244.0" prefWidth="544.0">
        <columns>
          <TableColumn fx:id="busIdCol" prefWidth="125.0" text="Bus ID" />
          <TableColumn fx:id="manufactureCol" prefWidth="161.0" text="Bus Manufacturer" />
            <TableColumn fx:id="busTypeCol" prefWidth="114.0" text="Bus Type" />
            <TableColumn fx:id="numberOfSeats" prefWidth="143.0" text="Number Of Seats" />
        </columns>
      </TableView>
      <Button layoutX="251.0" layoutY="411.0" mnemonicParsing="false" onAction="#viewDetailsOnClick" prefHeight="25.0" prefWidth="161.0" text="Select &amp; View Details">
