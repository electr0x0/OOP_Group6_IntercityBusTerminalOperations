<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="496.0" prefWidth="619.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.busterminal.controller.driver.TripHistoryController">
   <children>
      <Label layoutX="215.0" layoutY="40.0" text="Trip History">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewTripHistory" layoutX="77.0" layoutY="98.0" prefHeight="247.0" prefWidth="471.0">
        <columns>
          <TableColumn fx:id="dateCol" prefWidth="127.0" text="Date" />
          <TableColumn fx:id="routeCol" prefWidth="208.0" text="Route" />
            <TableColumn fx:id="statusCol" prefWidth="145.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="520.0" layoutY="457.0" mnemonicParsing="false" onAction="#switchToDashboardOnClick" prefHeight="25.0" prefWidth="89.0" text="Back" />
      <Button layoutX="75.0" layoutY="359.0" mnemonicParsing="false" onAction="#viewDetailsOnClick" prefHeight="25.0" prefWidth="110.0" text="View Details" />
      <Button layoutX="366.0" layoutY="357.0" mnemonicParsing="false" onAction="#confirmTripCompletedOnClick" prefHeight="55.0" prefWidth="180.0" text="Click to Confirm as Trip Completed" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="75.0" layoutY="403.0" mnemonicParsing="false" onAction="#deleteOnClick" prefHeight="25.0" prefWidth="110.0" text="Delete" />
   </children>
</AnchorPane>
